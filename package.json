{
  "name": "express-ts-starter",
  "version": "2.0.0",
  "scripts": {
    "start": "tsc && ts-node -r tsconfig-paths/register dist/index.js",
    "migrate": "ts-node-dev -r tsconfig-paths/register ./src/migrate.ts",
    "migrate:test": "cross-env NODE_ENV=test ts-node-dev -r tsconfig-paths/register ./src/migrate.ts",
    "dev": "ts-node-dev -r tsconfig-paths/register ./src/index.ts",
    "test": "cross-env NODE_ENV=test jest --runInBand"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "16.4.5",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.12.0",
    "sequelize": "^6.37.6",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/cors": "^2.8.17",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/supertest": "^6.0.3",
    "@types/node": "20.14.7",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.5.2"
  }
}
